<h2>{{mainTitle}}</h2>

<ul class="list-group">

  {{#if userInfo.admin}}
    <li class="list-group-item active" aria-current="true">Access: Admin</li>
  {{else}}
    <li class="list-group-item" aria-current="true">User</li>
  {{/if}}
  <li class="list-group-item">Email: {{userInfo.user}}</li>
  <li class="list-group-item">Name: {{userInfo.name}}</li>
  <li class="list-group-item">Age: {{userInfo.age}}</li>
</ul>